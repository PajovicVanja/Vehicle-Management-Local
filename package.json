{
  "name": "vehicle-management",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:frontend\"",
    "dev:server": "nodemon server/server.js",
    "dev:frontend": "npm --prefix frontend start",
    "build:frontend": "npm --prefix frontend ci && npm --prefix frontend run build",
    "test": "npm --prefix frontend test -- --watchAll=false",
    "test:automation:licence": "mocha automation:/TC02.js --reporter mochawesome --reporter-options reportDir=test-report,reportFilename=upload-test",
    "test:automation:adminadd": "mocha automation/TC03.js --reporter mochawesome --reporter-options reportDir=test-report,reportFilename=admin-test",
    "test:automation:driverreservation": "mocha automation/TC04.js --reporter mochawesome --reporter-options reportDir=test-report,reportFilename=driver-test",
    "test:automation:all": "mocha automation/*.js --reporter mochawesome --reporter-options reportDir=test-report,reportFilename=full-report,overwrite=true"
  },
  "dependencies": {
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.7.0",
    "formidable": "^3.5.0"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "concurrently": "^9.0.1",
    "mocha": "^11.7.5",
    "mochawesome": "^7.1.3",
    "nodemon": "^3.1.7",
    "selenium-webdriver": "^4.38.0"
  }
}